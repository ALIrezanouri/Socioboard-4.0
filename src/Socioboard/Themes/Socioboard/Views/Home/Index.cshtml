<body ng-model="lstProfiles">
    <input type="hidden" id="selectedGroupId" ng-init="groupId=@ViewBag.selectedGroupId" />
    <div   ng-init="Downgrade=@ViewBag.downgrade" ></div>
    <div  ng-init="MaxCount=@ViewBag.MaxCount" ></div>
    <div  ng-init="profileCount=@ViewBag.profileCount" ></div>
    <div ng-init="AccountType=@ViewBag.AccountType"></div>
    <div ng-init="groups=@ViewBag.groups"></div>
    <div ng-init="user=@ViewBag.user"></div>
    <div ng-init="lstProfiles=@ViewBag.groupProfiles"> </div>
    @if (TempData["fbPages"] != null)
    {
        <div ng-init="lstAddFbPages=@TempData["fbPages"]"></div>
    }
    @if (TempData["linPages"] != null)
    {
        <div ng-init="lstaddlinpages=@TempData["linPages"]"></div>
    }
    @if (TempData["Ganalytics"] != null)
    {
        <div ng-init="lstGanalytics=@TempData["Ganalytics"]"></div>
    }
    @if (TempData["Youtube"] != null)
    {
        <div ng-init="lstYoutube=@TempData["Youtube"]"></div>
    }
    <div data-ng-include="'../contents/socioboard/views/shared/header.html'" data-ng-controller="HeaderController"></div>
    <!-- END HEADER -->
    <!-- START MAIN -->
    <div id="main">
        <!-- START WRAPPER -->
        <div class="wrapper">
            <!-- START LEFT SIDEBAR NAV-->
            <div data-ng-include="'../contents/socioboard/views/shared/sidebar.html'" data-ng-controller="SidebarController"></div>
            <!-- END LEFT SIDEBAR NAV-->
            <!-- main contant -->
            <div ui-view class="fade-in-up"></div>
        </div>
        <!-- END WRAPPER -->
    </div>
    <!-- END MAIN -->
    <div data-ng-include="'../contents/socioboard/views/shared/footer.html'" data-ng-controller="FooterController"></div>
