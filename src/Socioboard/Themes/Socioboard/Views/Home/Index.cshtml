        @if (TempData["Error"] != null)
        {
            <script>
            $(document).ready(function () {
                alertify.set({ delay: 5000 });
                alertify.error("@Convert.ToString(TempData["Error"])");
            });
            </script>
        }
        @if (TempData["Success"] != null)
        {
            <script>
            $(document).ready(function () {
                alertify.set({ delay: 5000 });
