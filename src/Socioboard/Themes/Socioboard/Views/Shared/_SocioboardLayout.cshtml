    <!-- SignUp Modal -->
    <div id="SignUpModal" class="modal modal-fixed-footer" style="width: 600px;">
        <form onsubmit="myFunction(); return false;" id="RegistrationForm">
            <div class="modal-content">
                <h4 class="center">Create a free account</h4>
                <div class="row">
                    <div class="row">
                        <div class="input-field col s12 m12">
                            <div class="center">
                                <a id="fb_account"  href="#" onclick="googleLogin();">
                                    <img src="~/contents/socioboard/images/gplus_signin_button.png" class="responsive-img" alt="" data-pin-nopin="true">
                                </a><br>
                                <a id="fb_account" href="#" onclick="facebookLogin();">
                                    <img src="~/contents/socioboard/images/fb_signin_button.png" class="responsive-img" alt="" data-pin-nopin="true">
                                </a>
                            </div>
                        </div>
                        <div class="input-field col s12 m12">
                            <select id="AccountType" name="AccountType">
                                <optgroup label="Basic Plans">
                                    <option value="0">Basic(Free)</option>
                                    <option value="1">Standard</option>
                                    <option value="2">Premium</option>
                                    <option value="3">Deluxe</option>
                                </optgroup>
                                <optgroup label="Expert Plans">
                                    <option value="4">Topaz</option>
                                    <option value="5">Ruby</option>
                                    <option value="6">Gold</option>
                                    <option value="7">Platinum</option>
                                </optgroup>
                            </select>
                            <label>Choose your option</label>
                        </div>
                        <div class="input-field col s12 m12">
                            <select id="PaymentType" name="PaymentType">
                                <optgroup label="PaymentType">
                                    <option value="0">paypal</option>
                                    <option value="1">payumoney</option>
                                </optgroup>
                            </select>
                            <label>Choose your option</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s6">
                            <input id="FirstName" name="FirstName" type="text" required class="validate" pattern="^[a-zA-Z0-9]+$">
                            <label for="FirstName">First Name</label>
                        </div>
                        <div class="input-field col s6">
                            <input id="LastName" name="LastName" type="text" required class="validate" pattern="^[a-zA-Z0-9]+$">
                            <label for="LastName">Last Name</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="EmailId" name="EmailId" type="email" required class="validate" pattern="^([a-zA-Z0-9_\-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$">
                            <label for="EmailId" data-error="wrong" data-success="right">Email</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="PhoneNumber" name="PhoneNumber" type="tel" class="validate" title="Please enter number only" maxlength="10" pattern="[0-9]{10}">
                            <label for="PhoneNumber" data-error="PhoneNumber" data-success="right">Mobile</label>
                        </div>
                    </div>
                    <div class="row" style="display:none">
                        <div class="input-field col s12">
                            <input id="PhoneNumber" name="PhoneNumber" type="tel" class="validate" title="Please enter number only" maxlength="10" pattern="[0-9]{10}">
                            <label for="PhoneNumber" data-error="PhoneNumber" data-success="right">Mobile</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="Password" name="Password" type="password" title="One Capital letter,One Small letter,One number,One special character and min lenght must be 8" data-rule-minlength="8" required class="validate" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[$&+,:;=?@@#|'<>.^*()%!-_]).{8,}">
                            <label for="Password">Password</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="CnfPassword" name="CnfPassword" type="password" data-rule-minlength="8" required class="validate">
                            <label for="CnfPassword">Confirm Password</label>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">

                @*<div id="ImgLoading" class="btn btn-flat btn-primary ink-reaction hide"> <img class='img-portfolio img-responsive right' src='~/contents/socioboard/images/loading-spinner-grey.gif'></div>*@
                <button id="submitButton" type="submit" class="btn btn-flat btn-primary ink-reaction"> Register</button>

                <div></div>
            </div>
        </form>
    </div>



    <!-- SignInModal Structure -->
    <div id="SignInModal" class="modal">
        <div class="modal-content">
            <h4 class="center-align Righteous_font">Sign In</h4>
            <div class="col-md-12" id="fb-gp-login" style="display:none;">
                <div class="alert">
                    <strong style="font-weight:600">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp Please wait for your login</strong>
                    <img src="~/contents/socioboard/images/login_loader.gif" style="height:20px" />
                </div>
            </div>
            <hr>
            <div class="row center-align">
                <form onsubmit="Login(); return false;" class="col m6 s12">
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="logUserName" name="logUserName" type="email" class="validate" required onblur="retpwd();">
                            <label for="logUserName">Email</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="logPassword" name="logPassword" type="password" data-rule-minlength="6" required class="validate">
                            <label for="logPassword">Password</label>
                        </div>
                    </div>
                    <p>
                        <input type="checkbox" id="RememberMe" />
                        <label for="RememberMe">Remember me</label>
                    </p>
                    <p>Forgot password? <a class="modal-trigger modal-action modal-close modal-trigger" href="#ForgotPasswdModal" onclick="chng_btn_val_rev()">Click here to restore</a></p>

                    @*<img class='img-portfolio img-responsive hide' id="logLoading" src='~/contents/socioboard/images/loading-spinner-grey.gif'>*@

                    <div id="btnlogin">
                        <button type="submit" class="modal-action waves-effect waves-green btn">Submit</button>
                    </div>
                </form>
                <form class="col s12 m6">
                    <p>You can also register with your social account</p>
                    <div class="input-group">
                        <a id="fb_account" href="#" onclick="googleLogin();">
                            <img src="~/contents/socioboard/images/gplus_signin_button.png" class="responsive-img" alt="" data-pin-nopin="true">
                        </a><br>
                        <a id="fb_account" href="#" onclick="facebookLogin();">
                            <img src="~/contents/socioboard/images/fb_signin_button.png" class="responsive-img" alt="" data-pin-nopin="true">
                        </a>
                        <p>Not yet registered? &nbsp; <a class="modal-action modal-close modal-trigger" href="#SignUpModal">Register now</a></p>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="ForgotPasswdModal" class="modal">
        <form onsubmit="forgotPassword(); chng_btn_val(); return false;" class="col m6 s12">
            <div class="modal-content">
                <h4 class="Comfortaa_font">Forgot password</h4>
                <div class="row">
                    <div class="input-field col s12 box1">
                        <input id="forgotPwdEmail" name="forgotPwdEmail" type="email" required class="validate box_input" style="cursor:auto">
                        <label for="forgotPwdEmail">Email</label>
                    </div>

                </div>
            </div>

            @* For change button content after click.. *@
            <script>
                function chng_btn_val() {
                    document.getElementById("sub_btn").value = "Please Wait...";
                }

                function chng_btn_val_rev() {
                    document.getElementById("sub_btn").value = "Submit";
                }
            </script>

            <div class="modal-footer">
                @*<input type="submit" onclick="" id="sub_btn" class="waves-effect waves-light btn cyan" value="Submit" />@*Submit</input>*@
                <button id="sub_btn" type="submit" class="waves-effect waves-light btn cyan" value="submit">Submit</button>
            </div>
        </form>
    </div>


